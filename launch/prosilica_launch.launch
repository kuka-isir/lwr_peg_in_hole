<launch>

  <rosparam param="prosilica_driver/auto_gain">false</rosparam>

  <include file="$(find prosilica_camera)/launch/generic.launch">
    <arg name="camera"       value="prosilica" />
    <arg name="ip_address"   value="" />
    <arg name="trigger_mode" value="streaming" />
    <arg name="frame_id"     value="head_cam_link_optical" />
  </include>

  <node ns="/prosilica/" name="rect_prosilica" pkg="image_proc" type="image_proc" output="screen" />

</launch>
